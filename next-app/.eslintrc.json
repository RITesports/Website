{
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "next/core-web-vitals"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "max-len": "off",
    "react/jsx-props-no-spreading": "off"
  }
}